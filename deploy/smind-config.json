{
  "client_pool": {
    "name": "redis",
    "cfg": {
      "host": "rmain",
      "port": 6379,
      "passwd": "",
      "prefix": "nlpapi",
      "path": "rmain"
    }
  },
  "data_store": {
    "name": "redis",
    "cfg": {
      "host": "rbody",
      "port": 6379,
      "passwd": "",
      "prefix": "nlpapi",
      "path": "rbody"
    },
    "mode": "size"
  },
  "executor_manager": {
    "name": "thread",
    "batch_size": 8,
    "parallelism": 1,
    "sleep_on_idle": 0.1,
    "reclaim_sleep": 600.0
  },
  "queue_pool": {
    "name": "redis",
    "cfg": {
      "host": "rdata",
      "port": 6379,
      "passwd": "",
      "prefix": "nlpapi",
      "path": "rdata"
    },
    "check_assertions": false
  },
  "graph_cache": {
    "name": "redis",
    "cfg": {
      "host": "rcache",
      "port": 6379,
      "passwd": "",
      "prefix": "nlpapi",
      "path": "rcache"
    }
  },
  "strategy": {
    "node": {
      "name": "dedicated"
    },
    "queue": {
      "name": "simple"
    }
  },
  "readonly_access": {
    "name": "ram",
    "scratch": "/smind_cache/"
  },
  "logger": {
    "listeners": [
      {
        "name": "stdout",
        "show_debug": true
      }
    ],
    "disable_events": ["measure", "debug"]
  }
}
